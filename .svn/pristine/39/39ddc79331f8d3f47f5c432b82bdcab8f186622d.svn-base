body{
    background: #f9f5f5;
}
.videoPlayer{
    height:4.2rem;
    margin-top: 0.8rem;
    position: relative;
}
.videoWrap{
    background: #f9f5f5;
}
.videoWrap video{
    width: 100%;
    height: 100%;
    /*background-image: url("../images/video.jpg");*/
    /*background-size: 100% 100%;*/
    /*background-repeat: no-repeat;*/
    /*background-position: center;*/
    position: relative;
}
.videoWrap  img{
    position: absolute;
    left: 50%;
    top:50%;
    margin-top: -0.44rem;
    margin-left: -0.44rem;
    width: 0.88rem;
    height: 0.88rem;
}

.videoTab{
    height: 0.8rem;
    line-height: 0.8rem;
    font-size: 0;
    background: #ffffff;
    border-bottom: 0.02rem solid #f9f5f5;
}
.videoTab p{
    display: inline-block;
    width: 50%;
    text-align: center;
    font-size: 0.32rem;
    color: #999999;
}
.videoTab p.active span{
    display: inline-block;
    height: 0.8rem;
    color:#e83329 ;
    padding: 0 0.3rem;
    border-bottom: 0.02rem solid #e83329;
}
.videoContent{
   display: none;
}
.videoContent.active{
  display: block;
}
.title{
    height: 1rem;
    line-height: 1rem;
    border-bottom: 0.02rem solid #f9f5f5;
}
.title .left{
    font-size: 0.32rem;
    color: #333333;
    position: relative;
    padding-left: 0.63rem;
    overflow: hidden;
    text-overflow: ellipsis;
    height: 0.9rem;

    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;


}
.title .left:after{
    position: absolute;
    content: '';
    left: 0;
    top:50%;
    width: 0.45rem;
    height: 0.41rem;
    background-image: url("../images/star.png");
    background-size: 0.45rem 0.41rem;
    background-repeat: no-repeat;
    margin-top: -0.2rem;

}
.title .right{
    font-size: 0.26rem;
    color: #e32b2c;
}
.videoList{
    padding-left: 0.63rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    justify-content: space-between;
    padding-top: 0.2rem;
    padding-bottom: 0.15rem;
}
.videoLeft .videoTotalSum{
    font-size: 0.28rem;
    color: #333333;

}
.videoLeft .videoTime{
    color: #999999;
    font-size: 0.22rem;
}
.videoRight p{
    font-size: 0.26rem;
    color: #999;
    width: 1rem;
    margin-left: 0.2rem;
}
.addBtn,.addBtn2{
    text-align: center;
    margin: 0.46rem 0 0.46rem 0;

}
.videoItem{
    padding: 0 0.32rem;
    background: #fff;
}
.addBtn span,.addBtn2 span{
    display: inline-block;
    width: 5.3rem;
    height: 0.72rem;
    line-height: 0.72rem;
    background: #e83329;
    font-size: 0.32rem;
    color: white;
    border-radius: 0.3rem;
    -webkit-border-radius: 0.3rem;
}
.addBtn2 span{
    background: #999999;
}

.videoConentInto{
    padding-top: 0.3rem;
    padding-bottom: 0.2rem;
}
.videoConentInto p{
    text-indent: 2em;
    font-size: 0.26rem;
    color: #333333;
    padding-bottom: 0.2rem;
    text-align: justify;
}
