.header{
  height: 0.8rem;
  background: #e83329;
  line-height: 0.8rem;
  /*font-size: 0.36rem;*/
  font-size: 18px;
  color: #ffffff;
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  width: 100%;
  max-width: 7.5rem;
  margin: 0 auto;
  z-index: 999;
}
a:hover,a:link,a:visited{
  color: #333;
}
.header span.left{
  padding-left: 0.32rem;
}
.header span img{
  display: inline-block;
  width: 0.4rem;
  height: 0.34rem;
  vertical-align: middle;
  padding-right: 0.32rem;
}

.newsItem{
  position: relative;
  height: auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  /* -webkit-box-align: center; */
  /* -webkit-align-items: center; */
  /* align-items: center; */
  padding: 0.2rem 0;
}
.newsItem .newsItemImg{
  width: 2rem;
  max-width: 150px;
}
.newsItem .newsItemImg img{
  display: block;
  float: right;
}
.newsItem .newsItemText{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  min-width: 0;
  padding-left: 0.2rem;
  height: auto;
}
.newsItem .newsItemText .newsItemTitle{
  /*font-size: 17px;*/
  /*font-size: 0.3rem;*/
  font-size: 14px;
  color: #333333;
  margin-right: 0.2rem;
  line-height: 1.2;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-weight: 500;
}

.newsItem .newsItemText .newsItemContent{
  /*font-size: 13px;*/
  /*font-size: 0.26rem;*/
  font-size: 12px;
  color: #666666;
  line-height: 0.34rem;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-right: 0.2rem;
  height: 0.66rem;
  margin-top: 0.15rem;
}
/*flex*/
.flex{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;

}
.clamp2{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.flexAuto{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  min-width: 0;
}
/*loading*/
.weui-loadmore,.loadMore {
  width: 65%;
  margin: 0.15rem auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
  color: #808080;
}
.weui-loadmore{

}


@-webkit-keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
  color: #808080;
}
.weui-loadmore_line {
  border-top: 1px solid #E5E5E5;
  /*margin-top: 0.2rem;*/
  margin-top: 0.4rem;
}
.weui-loadmore_line .weui-loadmore__tips {
  position: relative;
  top: -0.9em;
  padding: 0 .55em;
  background-color: #FFFFFF;
  color: #808080;

}

.weui-toast {
  position: fixed;
  z-index: 5000;
  width: 7.6em;
  min-height: 7.6em;
  top: 180px;
  left: 50%;
  margin-left: -3.8em;
  background: rgba(17,17,17,0.7);
  text-align: center;
  border-radius: 5px;
  color: #FFFFFF;
}
@-webkit-keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

.weui-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .6);
}

.weui-mask_transparent{
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
}
.weui-icon_toast.weui-loading {
  margin: 30px 0 0;
  width: 38px;
  height: 38px;
  vertical-align: baseline;
}
.weui-loading {
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;


}
