body,html{
    height: 100%;
    max-width: 750px;
    margin: 0 auto;
    overflow: scroll;
      background-image: url(../images/bannerIndex.jpg);
    background-position: center;
    background-size: 7.5rem 100%;
    background-repeat: no-repeat;
}
.container{
    width: 100%;
    height: 100%;
    background-image: url(../images/bannerIndex.jpg);
    background-position: center;
    background-size: 7.5rem 100%;
    background-repeat: no-repeat;
    overflow: hidden;
    padding-bottom: 0;
    position: relative;
}
.footerFixed{
    position: absolute;
    width: 5.4rem;
    left: 1rem;
    bottom: 0.2rem;
}
.formBtn{
    margin-bottom: 0.36rem
}
.formBtn input{
    background: #1e65e8 !important;
    
}
.footerFixed p{
    text-align: center;
    font-size: 0.26rem;
    color: #666666
}